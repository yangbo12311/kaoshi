<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yangbo28.info">
    <!-- 访问网络，网络定位需要上网-->
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:name="com.yangbo28.info.JkbdApplication"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:testOnly="false"
        android:theme="@style/AppTheme">
        <activity android:name="com.yangbo28.info.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yangbo28.info.MainActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.yangbo28.info.WebDetailActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.yangbo28.info.NickActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>